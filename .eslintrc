{
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "env": {
        "browser": true,
        "node": true,
        "es6": true
  },
  "rules": {
      "no-else-return": "off",
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "arrow-parens": ["error", "as-needed"],
      "comma-dangle": ["error", "never"]
  },
}
